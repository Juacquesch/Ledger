<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import java.net.URL?>
<fx:root fx:id="addAcctGridPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
         minHeight="-Infinity" minWidth="-Infinity" style="-fx-background-color: #ffffff;" type="GridPane"
         xmlns="http://javafx.com/javafx/8.0.76-ea" xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints hgrow="ALWAYS" maxWidth="1.7976931348623157E308" minWidth="-Infinity"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308" minWidth="-Infinity"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="40.0" minHeight="3.0" prefHeight="40.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="60.0" minHeight="10.0" prefHeight="60.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="60.0" minHeight="10.0" prefHeight="60.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="60.0" minHeight="10.0" prefHeight="60.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="60.0" minHeight="10.0" prefHeight="60.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <Pane fx:id="header" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
              GridPane.columnSpan="3" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS">
            <children>
                <Text fx:id="title" layoutX="14.0" layoutY="29.0" strokeType="OUTSIDE" strokeWidth="0.0"
                      text="Add Account">
                    <font>
                        <Font name="Calibri" size="28.0"/>
                    </font>
                </Text>
            </children>
        </Pane>
        <Text fx:id="accountNameLabel" strokeType="OUTSIDE" strokeWidth="0.0" text="Account Name:" textAlignment="RIGHT"
              GridPane.halignment="RIGHT" GridPane.rowIndex="1">
            <font>
                <Font name="Calibri Light" size="22.0"/>
            </font>
        </Text>
        <TextField fx:id="accountNameText" maxHeight="30.0" maxWidth="200.0" prefHeight="30.0" prefWidth="200.0"
                   GridPane.columnIndex="1" GridPane.rowIndex="1">
            <GridPane.margin>
                <Insets left="15.0" right="15.0"/>
            </GridPane.margin>
        </TextField>
        <Text fx:id="accountDescLabel" strokeType="OUTSIDE" strokeWidth="0.0" text="Description:" textAlignment="RIGHT"
              GridPane.halignment="RIGHT" GridPane.rowIndex="2">
            <font>
                <Font name="Calibri Light" size="22.0"/>
            </font>
        </Text>
        <TextField fx:id="accountDescription" maxHeight="30.0" maxWidth="200.0" prefHeight="30.0" prefWidth="200.0"
                   GridPane.columnIndex="1" GridPane.rowIndex="2">
            <GridPane.margin>
                <Insets left="15.0"/>
            </GridPane.margin>
        </TextField>
        <Text fx:id="startingAmtLabel" strokeType="OUTSIDE" strokeWidth="0.0" text="Starting Amount:"
              textAlignment="RIGHT" GridPane.halignment="RIGHT" GridPane.rowIndex="3">
            <font>
                <Font name="Calibri Light" size="22.0"/>
            </font>
            <GridPane.margin>
                <Insets left="15.0"/>
            </GridPane.margin>
        </Text>
        <TextField fx:id="accountAmtText" maxHeight="30.0" maxWidth="200.0" prefHeight="30.0" prefWidth="200.0"
                   GridPane.columnIndex="1" GridPane.rowIndex="3">
            <GridPane.margin>
                <Insets left="15.0"/>
            </GridPane.margin>
        </TextField>
        <Button fx:id="submitAccountInfo" mnemonicParsing="false" prefHeight="30.0" prefWidth="200.0"
                text="Submit as Complete" GridPane.columnSpan="3" GridPane.halignment="CENTER" GridPane.rowIndex="4">
            <GridPane.margin>
                <Insets/>
            </GridPane.margin>
            <font>
                <Font name="Calibri Light" size="16.0"/>
            </font>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </Button>
    </children>
    <stylesheets>
        <URL value="@../css/colorStyle.css"/>
    </stylesheets>
</fx:root>
