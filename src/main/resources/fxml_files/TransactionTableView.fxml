<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>

<fx:root editable="true" type="TableView" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <columns>
      <TableColumn fx:id="amountColumn" prefWidth="150.0" text="Amount" />
      <TableColumn fx:id="dateColumn" minWidth="0.0" prefWidth="150.0" text="Date" />
      <TableColumn fx:id="payeeColumn" minWidth="0.0" prefWidth="150.0" text="Payee" />
      <TableColumn fx:id="typeColumn" minWidth="0.0" prefWidth="150.0" text="Type" />
      <TableColumn fx:id="categoryColumn" minWidth="0.0" prefWidth="150.0" text="Category" />
      <TableColumn fx:id="clearedColumn" minWidth="0.0" prefWidth="150.0" text="Cleared" />
   </columns>
   <columnResizePolicy>
      <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
   </columnResizePolicy>
</fx:root>
